<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context = "http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation=
	"http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">


	<bean id="emp1" class="com.Natwest.Employee">
	
		<constructor-arg value="12" type="int"></constructor-arg>
		<constructor-arg value="Sonoo" ></constructor-arg>
		<constructor-arg value="pune" ></constructor-arg>
		
		<property name="id" value="101" />
		<property name="name" value="Kiran" />
		<property name="city" value="pune" /> 
	</bean>
	
	<bean id="emp2" class="com.Natwest.Employee">
		<constructor-arg value="12" type="int"></constructor-arg>
		<constructor-arg value="Sonoo" ></constructor-arg>
		<constructor-arg value="pune" ></constructor-arg>
	</bean>
	<bean id="emp3" class="com.Natwest.Employee">
		<property name="id" value="101" />
		<property name="name" value="Kiran" />
		<property name="city" value="pune" />
	</bean>
	<context:component-scan base-package="com.Natwest"></context:component-scan>
	 
</beans>
                